<?xml version="1.0"?>
<launch>

	<!-- Launch world -->
	<include file="$(find Risk-based-Stochastic-Control-Barrier-Functions-)/launch/Tjunction_demo_world.launch">
	</include>

   	<!-- Launch cbf controller monitor (output console)-->
	<node name="cbf_controller" pkg="cbf_ros" type="cbf_controller.py" output="screen">
	</node>

	<!-- agent controller -->
	<node pkg="Risk-based-Stochastic-Control-Barrier-Functions-" type="agent.py" name="agent1_controller" args="--model_name agent1">
	</node>
	<node pkg="Risk-based-Stochastic-Control-Barrier-Functions-" type="agent.py" name="agent2_controller" args="--model_name agent2">
	</node>

</launch>
